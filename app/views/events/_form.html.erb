<%= form_with model: event, local: true do |form| %>

  <div class="form-group">
    <%= form.label :"日時" %><br>
    <%= form.text_field :datetime, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :"メンバー" %><br>
    <%= form.text_field :member, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :"場所" %><br>
    <%= form.text_field :place, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :"部位" %><br>
    <%= form.text_field :part, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :"登録者" %><br>
    <%= form.text_field :user_id, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :"詳細" %><br>
    <%= form.text_field :description, class: "form-control" %>
  </div>
  <%= form.submit class: "btn btn-default" %>
<% end %>
